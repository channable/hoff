#!/bin/bash
#
# Converts the ".dia" diagram files in doc/ to ".svg" files.
#
# Needs dia installed, use `apt install dia` to install.

dia -e doc/merge-train.{svg,dia}
dia -e doc/merge-train-failure.{svg,dia}
dia -e doc/merge-train-restart.{svg,dia}
dia -e doc/merge-train-rebase-failure-1.{svg,dia}
dia -e doc/merge-train-rebase-failure-2.{svg,dia}
dia -e doc/merge-train-delay.{svg,dia}
dia -e doc/no-merge-train.{svg,dia}
